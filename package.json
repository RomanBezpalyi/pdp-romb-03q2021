{
  "name": "@ciklum-digital/cra-template-typescript-redux",
  "version": "0.3.0",
  "license": "MIT",
  "author": "Roman Bezpalyi",
  "private": false,
  "engines": {
    "node": ">=10"
  },
  "main": "template.json",
  "description": "CRA template for Ciklum-digital. A quick start Create React App template with React Router, Redux, TypeScript, ESlint, Prettier configurations",
  "keywords": [
    "react",
    "create-react-app",
    "cra-template",
    "template",
    "eslint",
    "react-router",
    "react-router-dom",
    "typescript",
    "prettier"
  ],
  "files": [
    "template",
    "template.json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/RomanBezpalyi/pdp-romb-03q2021.git",
    "directory": "/"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "clean-files": "rm -rf ./template/public ./template/src && rm -f ./template/.eslintignore ./template/.eslintrc  ./template/.prettierrc ./template/README.md ./template/README_CRA.md ./template/tsconfig.json",
    "copy-files": "cp -a ./src/. template/src && cp -a ./public/. template/public && cp .eslintignore .eslintrc .prettierrc tsconfig.json README.md README_CRA.md template/",
    "prepublishOnly": "yarn clean-files && yarn copy-files",
    "commit": "npx git-cz",
    "release": "npx semantic-release",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./",
    "fix": "eslint --ext .js,.jsx,.ts,.tsx  --fix ./"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "tag": "latest",
    "access": "public"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "bootstrap": "^5.0.2",
    "bootstrap-icons": "^1.5.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "react": "^16.12.0",
    "react-color": "^2.19.3",
    "react-dom": "^16.12.0",
    "react-query": "^3.23.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^4.0.3",
    "react-toastify": "^8.0.2",
    "reactstrap": "^8.10.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^3.0.6",
    "@semantic-release/git": "^8.0.0",
    "@types/enzyme": "^3.10.4",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.9.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-color": "^3.0.5",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^7.31.0",
    "eslint-config-airbnb-typescript": "^6.3.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "node-sass": "^6.0.1",
    "prettier": "^1.19.1",
    "typescript": "~3.7.2"
  }
}
